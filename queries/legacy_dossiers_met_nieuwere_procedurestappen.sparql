PREFIX besluit: <http://data.vlaanderen.be/ns/besluit#>
PREFIX dossier: <https://data.vlaanderen.be/ns/dossier#>
PREFIX besluitvorming: <http://data.vlaanderen.be/ns/besluitvorming#>
PREFIX ext: <http://mu.semte.ch/vocabularies/ext/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX dct: <http://purl.org/dc/terms/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX tl: <http://mu.semte.ch/vocabularies/typed-literals/>
PREFIX pub: <http://mu.semte.ch/vocabularies/ext/publicatie/>
PREFIX core: <http://mu.semte.ch/vocabularies/core/>

SELECT DISTINCT  ?dossier ?uuid ?kaleidosUri WHERE {
  GRAPH <http://mu.semte.ch/graphs/organizations/kanselarij> {
    ?dossier core:uuid ?uuid .
    ?dossier dossier:doorloopt ?procedurestap .
    ?procedurestap ext:isAangevraagdVoor ?meeting .
    ?meeting besluit:geplandeStart ?plannedStart .
    FILTER ( ?plannedStart < xsd:dateTime("2019-10-02T02:00:00Z"))
    ?dossier dossier:doorloopt ?procedurestap2 .
    ?procedurestap2 ext:isAangevraagdVoor ?meeting2 .
    ?meeting2 besluit:geplandeStart ?plannedStart2 .
    FILTER ( ?plannedStart2 >= xsd:dateTime("2019-10-02T02:00:00Z"))
    BIND(CONCAT("https://kaleidos-test.vlaanderen.be/dossiers/", STR(?uuid), "/deeldossiers") as ?kaleidosUri)
  }
}
