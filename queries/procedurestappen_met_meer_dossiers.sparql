PREFIX besluit: <http://data.vlaanderen.be/ns/besluit#>
PREFIX dossier: <https://data.vlaanderen.be/ns/dossier#>
PREFIX besluitvorming: <http://data.vlaanderen.be/ns/besluitvorming#>
PREFIX ext: <http://mu.semte.ch/vocabularies/ext/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX dct: <http://purl.org/dc/terms/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX tl: <http://mu.semte.ch/vocabularies/typed-literals/>

SELECT DISTINCT ?procedurestap ?count ?source ?dossier ?dossierTitle ?procedurestapTitle WHERE {
  {
    SELECT COUNT(DISTINCT ?dossier) as ?count ?procedurestap WHERE {
      ?procedurestap a dossier:Procedurestap .
      ?dossier dossier:doorloopt ?procedurestap .
    }
  }
  FILTER (?count > 1)
  ?dossier dossier:doorloopt ?procedurestap .
  ?dossier dct:title ?dossierTitle .
  ?procedurestap dct:title ?procedurestapTitle .
  ?procedurestap dct:source ?source .
} ORDER BY desc(?count) ?procedurestap ?dossier ?source
